package test;

import java.util.Scanner;
 
class hello
{
  public static void main(String args[])
  {
    int c;
    System.out.println("Input an integer");
    Scanner s = new Scanner(System.in);
    c = s.nextInt();
 
    if ( (c/2)*2 == c )
      System.out.println("Even");
    else
      System.out.println("Odd");
  }}
